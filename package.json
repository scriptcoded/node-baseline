{
  "name": "elma-core",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node ./server.js | .\\node_modules\\.bin\\bunyan",
    "start-prod": "node ./server.js | ./node_modules/.bin/bunyan",
    "start-qa": "export DEBUG=elma-core:* & node ./server.js | ./node_modules/.bin/bunyan",
    "dev": "run-script-os",
    "dev:linux:darwin": "DEBUG=elma-core:* & nodemon ./server.js | ./node_modules/.bin/bunyan",
    "dev:win32": "SET DEBUG=elma-core:* & nodemon ./server.js | .\\node_modules\\.bin\\bunyan",
    "test": "run-script-os",
    "test:linux:darwin": "standard && export NODE_ENV=test ./node_modules/.bin/mocha --reporter spec | .\\node_modules\\.bin\\bunyan",
    "test:win32": "standard && set NODE_ENV=test&& .\\node_modules\\.bin\\mocha --exit --reporter spec"
  },
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "bunyan-mongodb-stream": "0.0.3",
    "chai": "^4.1.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "express-validator": "^5.0.0",
    "formidable": "^1.2.1",
    "jsonpath": "^1.0.0",
    "jsonwebtoken": "^8.1.1",
    "jwt-decode": "^2.2.0",
    "mocha": "^5.0.5",
    "moment": "^2.22.0",
    "mongoose": "^5.0.4",
    "morgan": "^1.9.0",
    "node-mocks-http": "^1.5.8",
    "node-version-compare": "^1.0.1",
    "nodemailer": "^4.6.4",
    "npm": "^5.8.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "request": "^2.85.0",
    "run-script-os": "^1.0.3",
    "shelljs": "^0.8.2",
    "sinon": "^4.4.10",
    "sprintf-js": "^1.1.1",
    "standard": "^11.0.1",
    "supertest": "^3.0.0",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@angular/cli": "1.6.7",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.14.12",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
